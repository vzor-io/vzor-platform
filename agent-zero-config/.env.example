# Agent Zero VZOR Configuration
# Copy to .env and fill in real values

A0_PERSISTENT_RUNTIME_ID=<generate-random-hex>
ROOT_PASSWORD=<root-password>

# Allowed origins for CSRF
ALLOWED_ORIGINS=*://localhost,*://localhost:*,*://127.0.0.1,*://127.0.0.1:*,http://YOUR_SERVER_IP,http://YOUR_SERVER_IP:*,https://YOUR_DOMAIN

# Timezone
DEFAULT_USER_UTC_OFFSET_MINUTES=180
DEFAULT_USER_TIMEZONE=Etc/GMT-3

# API Keys
DEEPSEEK_API_KEY=<your-deepseek-key>
GOOGLE_API_KEY=<your-google-key>
GEMINI_API_KEY=<your-gemini-key>
GEMINI_API_BASE=<your-gemini-base-url>

# Model settings (override defaults)
A0_SET_chat_model_provider=deepseek
A0_SET_chat_model_name=deepseek-chat
A0_SET_util_model_provider=deepseek
A0_SET_util_model_name=deepseek-chat
A0_SET_embed_model_provider=huggingface
A0_SET_embed_model_name=sentence-transformers/all-MiniLM-L6-v2

# MCP Server Token (must match vzor-api X-API-KEY)
MCP_SERVER_TOKEN=vzor-agent-key-2026

# API Key aliases for LiteLLM
API_KEY_DEEPSEEK=<same-as-DEEPSEEK_API_KEY>
API_KEY_GOOGLE=<same-as-GOOGLE_API_KEY>

# Auth (leave empty for no login)
AUTH_LOGIN=
AUTH_PASSWORD=
RFC_PASSWORD=
